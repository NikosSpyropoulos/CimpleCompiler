program whileLoop
    declare x;
    declare a;
    # main - x should be = 300 #
    {
        x := 0;
        a := 20;

        while ( x < 300 )
        {
            x := a + x;
            if (x < 50)
                x := x + 10;
            else
                a := a * 2;

        };

        x:= x - 30;
        print(x);

    }
	.
