program multiplication
declare x;
procedure multi (inout num)
{
    i:=1;
    incase
        case (i <= 10)
        {
            num := num * i;
            print(num);
        }
        case (i >= 50)
            i := i+10;
        case (i >=100)
            i := i - 20;
}
#main#
{
    input(x);
    call multi(inout x);
}.
